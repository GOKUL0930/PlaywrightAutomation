                                "part_name": {
                                    "type": "string",
                                    "example": "PartName"
                                },
                                "part_number": {
                                    "type": "integer",
                                    "example": "93705"
                                },
                                "quantity": {
                                    "type": "integer",
                                    "example": "100"
                                },
                                "unit_price": {
                                    "type": "integer",
                                    "example": "10.10"
                                },
                                "total_price": {
                                    "type": "integer",
                                    "example": "10.10"
                                },
                                "part_exists": {
                                    "type": "string",
                                    "example": "New"
                                },
                                "unit_quantity": {
                                    "type": "integer",
                                    "example": "20"
                                },
                                "material": {
                                    "type": "string",
                                    "example": "Liquid"
                                },
                                "part_file_url": {
                                    "type": "string",
                                    "example": "image_url"
                                }
                            },
